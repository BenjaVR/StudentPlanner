{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "whitespace": [
            true,
            "check-module"
        ],
        "member-ordering": [
            true,
            {
                "order": "fields-first"
            }
        ],
        "no-submodule-imports": [
            true,
            "moment/locale",
            "antd/lib/locale-provider"
        ]
    }
}
